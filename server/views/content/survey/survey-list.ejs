  <!-- Main Content -->
  <div class="container">
      <div class="row">
          <div class="offset-md-1 col-md-11">
              <h1><%= title %></h1>

              <a href="/survey-list/add" class="btn btn-primary" role="button">
                  <i class="fas fa-plus-circle"></i>Create a Survey</a>

              <br>
              <table class="table table-hover">
                  <thead class="table-dark">
                      <!-- Header Row-->
                      <tr>
                          <% if(typeof displayName == 'string' && displayName) { %>
                          <th class="text-center col-2">Title</th>
                          <th class="text-center col-3">Subtitle</th>
                          <th class="text-center col-5">Description</th>
                          <th class="text-center col-5">Edit</th>
                          <th class="text-center col-5">Delete</th>
                          <% } else { %>
                          <th class="text-center col-2">Title</th>
                          <th class="text-center col-3">Subtitle</th>
                          <th class="text-center col-5">Description</th>
                          <th class="text-center col-5">Edit</th>
                          <th class="text-center col-5">Delete</th>

                          <% } %>
                      </tr>

                      <!-- Repeatable Template Row -->

                      <% for(let count = 0; count < SurveyList.length; count++) { %>
                      <tr>

                          <td class="text-center col-2"><%= SurveyList[count].title %></td>
                          <td class="text-center col-3"><%= SurveyList[count].subtitle %></td>
                          <td class="text-center col-5"><%= SurveyList[count].description %></td>
                          <td class="text-center col-5"><%= SurveyList[count].edit %></td>
                          <td class="text-center col-5"><%= SurveyList[count].delete %></td>

                          <td class="text-center col-1">
                              <a href="/survey-list/edit/<%= SurveyList[count]._id %>" class="btn btn-primary btn-sm">
                                  <i class="fas fa-pencil-alt"></i> Update</a>
                          </td>
                          <td class="text-center col-1">
                              <a href="/survey-list/delete/<%= SurveyList[count]._id %>" class="btn btn-danger btn-sm">
                                  <i class="fas fa-trash-alt"></i> Delete</a>
                          </td>

                      </tr>
                      <% } %>
              </table>
          </div>
      </div>
  </div>